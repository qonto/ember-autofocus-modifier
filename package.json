{
  "name": "ember-autofocus-modifier",
  "version": "7.0.0-beta.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:qonto/ember-autofocus-modifier.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm --filter ember-autofocus-modifier build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "lint:types": "pnpm --filter '*' lint:types",
    "prepare": "pnpm build",
    "postinstall": "lefthook install",
    "release": "release-it",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-autofocus-modifier start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@release-it-plugins/workspaces": "^4.0.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "concurrently": "^8.2.1",
    "lefthook": "^1.5.5",
    "prettier": "^3.0.3",
    "release-it": "^15.11.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "volta": {
    "node": "18.14.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "pnpm": {
    "overrides": {
      "ember-source": "4.12.2"
    }
  }
}
