{
  "name": "ember-autofocus-modifier",
  "version": "1.1.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qonto/ember-autofocus-modifier.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "yarn lint:hbs && yarn lint:js",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "format": "yarn format:hbs && yarn format:js",
    "format:hbs": "yarn prettier --write **/*.hbs --parser=glimmer",
    "format:js": "yarn prettier --write **/*.js",
    "build": "ember build",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "release-it"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-modifier": "^2.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.13.0",
    "ember-try": "^1.2.1",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "lerna-changelog": "^1.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.1.1",
    "qunit-dom": "^1.5.0",
    "release-it": "^13.7.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking Change",
      "bug": ":bug: Bug Fixes",
      "dependencies": ":pushpin: Dependency",
      "documentation": ":pencil: Documentation",
      "enhancement": ":zap: Improvement Features",
      "internal": ":house_with_garden: Internal",
      "refactoring": ":shirt: Refactoring",
      "security": ":lock: Security Fixes"
    }
  }
}
