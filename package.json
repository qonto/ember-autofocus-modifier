{
  "name": "ember-autofocus-modifier",
  "version": "0.1.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qonto/ember-autofocus-modifier.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "yarn lint:hbs && yarn lint:js",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "format": "yarn format:hbs && yarn format:js",
    "format:hbs": "yarn prettier --write **/*.hbs --parser=glimmer",
    "format:js": "yarn prettier --write **/*.js",
    "build": "ember build",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "release-it"
  },
  "dependencies": {
    "ember-cli-babel": "^7.11.1",
    "ember-modifier": "^1.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.14.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^4.0.8",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.15.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^1.8.1",
    "ember-try": "^1.2.1",
    "eslint": "^6.6.0",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^11.0.0",
    "lerna-changelog": "^0.8.3",
    "loader.js": "^4.7.0",
    "prettier": "^1.19.1",
    "qunit-dom": "^0.9.0",
    "release-it": "^12.4.3"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
